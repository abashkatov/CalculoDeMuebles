<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% trans %}Welcome!{% endtrans %}{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body class="c-app">
{% include 'component/sidebar.html.twig' %}
<div class="c-wrapper">
    {% include 'component/header.html.twig' %}
    <div class="c-body">
        <main class="c-main">
            <div class="container-fluid">
                <div class="fade-in">
                    {% block body %}{% endblock %}
                </div>
            </div>
        </main>
    </div>
    <footer class="c-footer">
    </footer>
</div>
{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>